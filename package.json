{
  "name": "playground-vite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "cp .env.localhost .env && vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "deploy-dev": "cp .env.dev .env && npm run build && netlify deploy --site digitalauto-dev --prod -d dist && echo '' > .env",
    "deploy-prod": "cp .env.prod .env && npm run build && netlify deploy --site digitalauto --prod -d dist && echo '' > .env",
    "deploy-bcx": "cp .env.bcx .env && npm run build && netlify deploy --site digitalauto-bcx --prod -d dist && echo '' > .env",
    "deploy-all": "npm run deploy-dev && npm run deploy-prod && npm run deploy-bcx"
  },
  "dependencies": {
    "@agney/react-loading": "^0.1.2",
    "@eclipse-velocitas/velocitas-project-generator": "^2.0.1",
    "@emotion/react": "^11.8.1",
    "@emotion/styled": "^11.8.1",
    "@monaco-editor/react": "^4.3.1",
    "@mui/material": "^5.4.3",
    "@netlify/functions": "^1.2.0",
    "@sentry/react": "^6.19.7",
    "@sentry/tracing": "^6.19.7",
    "assert": "^2.0.0",
    "axios": "^0.27.2",
    "buffer": "^6.0.3",
    "chart.js": "^3.8.0",
    "chartjs-plugin-datalabels": "^2.0.0",
    "clsx": "^1.1.1",
    "dedent": "^0.7.0",
    "firebase": "^9.8.3",
    "firebase-admin": "^11.1.0",
    "indent-string": "^5.0.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^4.2.0",
    "react-d3-tree": "^3.3.2",
    "react-dom": "^18.2.0",
    "react-icons": "^4.6.0",
    "react-router-dom": "^6.2.1",
    "react-star-ratings": "^2.3.0",
    "react-use": "^17.3.2",
    "sass": "^1.58.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/assert": "^1.5.6",
    "@types/dedent": "^0.7.0",
    "@types/node": "^16.11.25",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "autoprefixer": "^10.4.13",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.2.7",
    "typescript": "^4.9.3",
    "vite": "^4.1.0"
  }
}
