{
    "name": "playground-vite",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "cp .env.dev .env && vite",
        "dev-vn": "cp .env.vndev .env && vite",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "deploy-dev": "cp .env.dev .env && npm run build && netlify deploy --site digitalauto-dev --prod -d dist && echo '' > .env",
        "deploy-prod": "cp .env.prod .env && npm run build && netlify deploy --site digitalauto --prod -d dist && echo '' > .env",
        "deploy-bcx": "cp .env.bcx .env && npm run build && netlify deploy --site digitalauto-bcx --prod -d dist && echo '' > .env",
        "deploy-xhub": "cp .env.xhub .env && npm run build && netlify deploy --site digitalauto-xhub --prod -d dist && echo '' > .env",
        "deploy-vndev": "cp .env.vndev .env && npm run build && netlify deploy --site playground-vndev --prod -d dist && echo '' > .env",
        "deploy-all": "npm run deploy-dev && npm run deploy-prod && npm run deploy-bcx",
        "fix:eslint": "eslint . --quiet --fix",
        "fix:prettier": "prettier --write \"{,!(node_modules|dist|dist-ssr)/**/}*.{js,jsx,ts,tsx,html,css}\""
    },
